@model MVCPanthers6jan2020.Models.EmpDeptModel
@{
    ViewBag.Title = "GetTestData4";
}

<h2>GetTestData4</h2>

<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model.emp)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
            
        </tr>
    }

</table>
<br />
<table>
    <tr>
        <th>DeptId</th>
        <th>DeptName</th>
         
    </tr>
    @foreach (var item in Model.dept)
    {
        <tr>
            <td>@item.DeptId</td>
            <td>@item.DeptName</td>
        </tr>
    }

</table>